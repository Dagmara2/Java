<GridPane alignment="center" hgap="10" prefHeight="459.0" prefWidth="532.0" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="sample.Controller">
