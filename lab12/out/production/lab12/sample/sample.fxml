            <ImageView fx:id="ogImg" fitHeight="185.0" fitWidth="250.0" layoutX="34.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="changedImg" fitHeight="185.0" fitWidth="250.0" layoutX="34.0" layoutY="232.0" pickOnBounds="true" preserveRatio="true" />
